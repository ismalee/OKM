<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns="urn:jbpm.org:jpdl-3.2"  name="forkJoinWorkflow">


	<start-state name="start-state1">
		<transition to="fork1"></transition>
	</start-state>


	<fork name="fork1">
		<transition to="task-node1"></transition>
		<transition to="task-node2" name="to task-node2"></transition>
	</fork>

	<task-node name="task-node1">
		<task name="task1">
			<assignment actor-id="okmAdmin"></assignment>
		</task>
		<transition to="join1" name="accept"></transition>
	</task-node>

	<task-node name="task-node2">
		<task name="task2">
			<assignment actor-id="josep"></assignment>
		</task>
		<transition to="join1" name="accept"></transition>
	</task-node>

	<join name="join1">
		<transition to="end-state1"></transition>
	</join>


	<end-state name="end-state1"></end-state>


</process-definition>